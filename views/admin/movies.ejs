<%- include('./header') %>

<div class="container-fluid">
  <h4>MOVIES</h4>
  <table class="table table-striped">
    <tr>
      <th>Poster:</th>
      <th>Title:</th>
      <th>Year:</th>
      <th>Action:</th>
    </tr>
    <% movies.forEach(function(movie){ %>
      <tr>
        <td><img src="<%= movie.poster %>" style="width: 50px; height: 50px; object-fit: cover;"></td>
        <td><%= movie.title.ka %></td>
        <td><%= movie.releaseYear %></td>
        <td><button type="button" class="btn btn-sm btn-danger">delete</button> </td>
      </tr>
		<% }) %>
  </table>
</div>


<%- include('./footer') %>
